---
import HandTrainerComponent from "../components/practice/hand-trainer.svelte";
import Layout from "../layouts/Layout.astro";
---

<head>
  {
    ["1", "2", "3", "4", "5", "6", "7", "8", "9", "T", "J", "Q", "K", "A"].map(
      (rank) =>
        ["H", "D", "C", "S"].map((suit) => (
          <link
            rel="preload"
            as="image"
            href={`/poker-cards/${rank}${suit}.svg`}
          />
        ))
    )
  }
</head>
<Layout>
  <HandTrainerComponent client:load />
</Layout>
